{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "homepage": "https://github.com/spember/backbone-vertebrae",
  "authors": [
    "<%= authorName %> <<%= authorEmail %>>"
  ],
  "description": "<%= appDescription %>",
  "main": "assets/js/app/init.js",
  "keywords": [
    "backbone",
    "templates"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "assets/js/lib",
    "test",
    "tests"
  ],
  "dependencies": {
    "underscore": "~1.5.2",
    "backbone": "~1.1.0",
    "requirejs": "2.1.10",<% if (jsLibrary == 'jQuery') { %>
    "jquery": "~2.1.0",<% } %><% if (jsLibrary == 'Zepto') { %>
    "zepto": "~1.0.0",<% } %><% if (includeFontAwesome) { %>
    "fontawesome": "~4.0.3",<% } %><% if (includeBootstrap) { %>
    "bootstrap": "~3.1.1",<% } %><% if (includeFastclick) { %>
    "fastclick": "~1.0.0",<% } %>
    "handlebars": "~1.0.0"
  },
  "devDependencies": {

  }
}
